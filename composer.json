{
    "name": "ddeboer/document-manipulation-bundle",
    "type": "symfony-bundle",
    "description": "Use this Symfony2 bundle to manipulate Microsoft Word and PDF documents",
    "keywords": [ "document", "doc", "pdf", "merge", "mailmerge" ],
    "homepage": "https://github.com/ddeboer/DdeboerDocumentManipulationBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "David de Boer",
            "email": "david@ddeboer.nl"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.zendframework.com/"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/framework-bundle": ">=2.1"
    },
    "require-dev": {
        "symfony/symfony": ">=2.1,<2.3-dev",
        "symfony/framework-bundle": ">=2.1",
        "zendframework/zendservice-livedocx": "2.*"

    },
    "suggest": {
        "zendframework/zendservice-livedocx": "If you have a LiveDocx account and want to use mail merging"
    },
    "autoload": {
        "psr-0": { "Ddeboer\\DocumentManipulationBundle": "" }
    },
    "target-dir": "Ddeboer/DocumentManipulationBundle"
}
